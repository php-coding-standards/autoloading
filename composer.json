{
    "name": "shake-php/autoloading",
    "description": "Autoloading made simple with Shake PHP.",
    "keywords": [
        "php72",
        "shake-php",
        "shake-autoloading",
        "composer-alternative",
        "secure-by-default",
        "elegant-coding",
        "psr-compliant",
        "classmap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish6657@gmail.com",
            "homepage": "https://github.com/inisik",
            "role": "Develop"
        }
    ],
    "autoload": {
        "classmap": [
            "./src"
        ]
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vimeo/psalm": "^1"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
